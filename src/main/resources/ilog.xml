<?xml version="1.0" encoding="UTF-8"?>
<!-- 如果是文件，那么要指定文件目录。文件实现提供者。文件名生成规则。每个文件大小 -->
<configuration>
    <!--日志输出实现提供者-->
	<property>
		<name>clog.logger</name>
		<value>ConsoleLogger</value>
	</property>
	<!--日志级别 i w e-->
	<property>
		<name>clog.level</name>
		<value>info</value>
	</property>
	<!-- 文件的目录 -->
	<property>
		<name>clog.dir</name>
		<value>/log/</value>
	</property>
	<!-- 文件的前缀 -->
	<property>
		<name>clog.file.pre</name>
		<value>test</value>
	</property>
	
	
	<property>
		<name>importsource.siamese.tickTime</name>
		<value>2000</value>
	</property>
	
	
	<!-- siamese 基本配置 -->
	<property>
		<name>importsource.siamese.tickTime</name>
		<value>2000</value>
	</property>
	<property>
		<name>importsource.siamese.initLimit</name>
		<value>5</value>
	</property>
	<property>
		<name>importsource.siamese.syncLimit</name>
		<value>2</value>
	</property>
	<property>
		<name>importsource.siamese.dataDir</name>
		<value>/Users/apple/siamese0/data</value>
	</property>
	<property>
		<name>importsource.siamese.dataLogDir</name>
		<value>/Users/apple/siamese0/logs</value>
	</property>
	<property>
		<name>importsource.siamese.clientPort</name>
		<value>4180</value>
	</property>
	<property>
		<name>importsource.siamese.servers</name>
		<value>
		server.0=127.0.0.1:8880:7770,    
        server.1=127.0.0.1:8881:7771,
        server.2=127.0.0.1:8882:7772
		
		</value>
	</property>
	
	 
 

</configuration>